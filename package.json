{
  "name": "soundbank",
  "version": "1.0.0",
  "description": "Django app for discovering anonymous songs",
  "main": "index.js",
  "scripts": {
    "preinstall": "pip3 install -r requirements.txt",
    "start": "python manage.py collectstatic --noinput && python.manage.py migrate && python manage.py runserver",
    "build": "./node_modules/node-sass/bin/node-sass static/sass/styles.sass > static/styles.css --output-style compressed",
    "watch": "./node_modules/node-sass/bin/node-sass --watch static/sass/styles.sass > static/styles.css --output-style compressed"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^4.5.0"
  }
}
